        <CopyFolder>Projects/GD32VF103 RVSTAR/dhrystone</CopyFolder>
        <RequiredComponent>common_base_resource</RequiredComponent>
        <RequiredComponent>common_base_include</RequiredComponent>
      </Project>
      <Project Name="RVSTAR-whetstone">
